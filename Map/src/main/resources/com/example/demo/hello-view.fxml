<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.HelloController">

    <Label text="City Sector Map: Double-Click Source (Green), Then Destination (Blue)">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Label>

    <Pane fx:id="mapPane" prefHeight="650.0" prefWidth="850.0"
          style="-fx-background-color: #f4f4f4; -fx-border-color: black; -fx-border-width: 2;"/>

</VBox>